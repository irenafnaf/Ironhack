<h1> <%= @project.name %></h1>
<ol>
	<% @time_entries.each do |time_entry| %>
		<li> 
		<%= time_entry.hours %> h - 
		<%= time_entry.minute %> m -
		<%= time_entry.date %>
		<%= link_to "Edit Time Entry", edit_project_time_entry_path(@project, time_entry) %>
		<%= link_to "Delete", project_time_entry_path(@project, time_entry), method: :delete, data: { confirm: 
			"Are you sure?"} %>
		</li>
	<%end%>
</ol>
		<%= link_to("Projects", projects_path) %>
		<%= link_to("New Time Entry", new_project_time_entry_path(@project)) %>