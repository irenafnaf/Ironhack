<%= form_for [@project, @time_entry] do |f| %>
	<%= f.label :hours %>
	<%= f.text_field :hours, size: 4 %>
	<br/>
	<%= f.label :minute %>
	<%= f.text_field :minute, size: 4%>
	<br/>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br/>
	<%= f.label :comments %>
	<%= f.text_area :comments %>
	<br/>

	<%= f.submit 'Save' %>
	<%= link_to 'Cancel', project_time_entries_path(@project) %>
<% end %>